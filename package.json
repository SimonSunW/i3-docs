{
  "name": "i3-docs",
  "version": "1.1.0",
  "description": "Awesome doc generation for LabVIEW projects",
  "scripts": {
    "test": "npm run lint",
    "deps": "bower install",
    "lint": "eslint .",
    "serve-dev": "http-server -p 8030 -o http://localhost:8030/web/index.html?i3-docs-path=../docs/ .",
    "serve-build": "http-server -p 8040 -o http://localhost:8040/docs/ .",
    "hash-win64-create": "hashdeep\\hashdeep64.exe -b lv\\Controls\\* lv\\Main\\* lv\\SubVI\\* > hashdeep.txt",
    "hash-win64-check": "hashdeep\\hashdeep64.exe -v -v -v -b -a -k hashdeep.txt lv\\Controls\\* lv\\Main\\* lv\\SubVI\\*",
    "hash-linux-check": "hashdeep -v -v -v -b -a -k hashdeep.txt lv/Controls/* lv/Main/* lv/SubVI/*",
    "prebuild-webapp": "mkdirp build",
    "build-webapp": "vulcanize --inline-scripts --inline-css --out-html build/index.html web/index.html",
    "i3-docs-install": "node bin/i3-docs.js install",
    "build-docs-win64": "npm run build-webapp && npm run i3-docs-install && npm run hash-win64-create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajsite/i3-docs.git"
  },
  "keywords": [
    "LabVIEW"
  ],
  "author": "Milan Raj (@rajsite)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rajsite/i3-docs/issues"
  },
  "homepage": "https://github.com/rajsite/i3-docs#readme",
  "devDependencies": {
    "bower": "^1.8.0",
    "crisper": "^2.0.2",
    "eslint": "^3.12.2",
    "http-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "vulcanize": "^1.15.2",
    "leaflet-sleep": "^0.5.1",
    "lunr": "^0.7.2",
    "promise-polyfill": "^6.0.2",
    "url-search-params": "^0.6.1",
    "whatwg-fetch": "^2.0.1"
  },
  "dependencies": {
    "find-parent-dir": "^0.3.0",
    "ini": "^1.3.4",
    "shelljs": "^0.7.5",
    "yargs": "^6.6.0"
  },
  "bin": {
    "i3-docs": "bin/i3-docs.js"
  }
}
